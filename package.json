{
  "name": "sub-dazzle",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "workspaces": [
    "electron",
    "react-client"
  ],
  "scripts": {
    "build": "tsc",
    "start": "concurrently 'npm run actions:electron' 'npm run actions:react'",
    "actions:electron": "cd electron && npm start",
    "actions:react": "cd react-client && node scripts/start.js",
    "server": "./bin/node.sh dist/index.js",
    "detect": "./bin/node.sh dist/examples/detect-subtitles.js"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "prettier": "^2.4.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "deepspeech": "^0.9.3",
    "express": "^4.17.1",
    "ffmpeg-static": "^4.4.0"
  }
}
